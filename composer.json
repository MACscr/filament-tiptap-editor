{
    "name": "awcodes/filament-tiptap-editor",
    "type": "package",
    "description": "A Tiptap ingtegration for Filament Admin/Forms.",
    "keywords": [
        "framework",
        "laravel",
        "filament",
        "tiptap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "^2.0",
        "intervention/image": "^2.7",
        "spatie/laravel-package-tools": "^1.9.2",
        "ueberdosis/tiptap-php": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "FilamentTiptapEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilamentTiptapEditor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentTiptapEditor\\FilamentTiptapEditorServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
